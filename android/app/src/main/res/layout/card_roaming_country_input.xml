<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_alignParentStart="true"
	android:layout_alignParentTop="true"
	android:layout_marginTop="23dp"
	android:focusableInTouchMode="true"
	android:focusable="true"
	android:padding="@dimen/default_padding_horizontal">

	<TextView
		android:id="@+id/country_input_label"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:text="enter:"
		android:textColor="@color/blackNormal"
		android:textStyle="bold" />

	<ro.vodafone.mcare.android.custom.CustomAutoCompleteEditText
		android:id="@+id/country_input"
		android:layout_width="match_parent"
		android:layout_height="@dimen/login_page_view_elements_height"
		android:layout_below="@+id/country_input_label"
		android:layout_marginTop="5dp"
		android:background="@drawable/gray_default_input_border"
		android:completionThreshold="1"
		android:inputType="text"
		android:maxLines="1"
		android:paddingLeft="14dp"
		android:paddingRight="14dp"
		android:textColor="#000000"
		android:textColorHint="@color/gray_text_color"
		android:textCursorDrawable="@null"
		android:textSize="@dimen/login_page_labels_text_size" />

	<ro.vodafone.mcare.android.ui.views.buttons.VodafoneButton
		android:id="@+id/country_input_submit_button"
		style="@style/CardPrimaryButton"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_below="@+id/country_input"
		android:layout_marginTop="20dp"
		android:enabled="false"
		android:text="AdministreazÄƒ serviciile"
		android:textSize="@dimen/button_text_size" />

</RelativeLayout>